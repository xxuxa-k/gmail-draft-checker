{
  "timeZone": "Asia/Tokyo",
  "exceptionLogging": "STACKDRIVER",
  "runtimeVersion": "V8",
  "dependencies": {
    "libraries": [
      {
        "userSymbol": "dayjs",
        "version": "1",
        "libraryId": "1ShsRhHc8tgPy5wGOzUvgEhOedJUQD53m-gd8lG2MOgs-dXC_aCZn9lFB"
      }
    ]
  },
  "oauthScopes": [
    "https://mail.google.com/",
    "https://www.googleapis.com/auth/drive",
    "https://www.googleapis.com/auth/gmail.compose",
    "https://www.googleapis.com/auth/gmail.addons.execute",
    "https://www.googleapis.com/auth/gmail.addons.current.action.compose",
    "https://www.googleapis.com/auth/gmail.addons.current.message.metadata"

  ],
  "addOns": {
    "common": {
      "name": "Gmail Draft Checker",
      "logoUrl": "http://placehold.jp/20/3d4070/ffffff/24x24.png?text=%E9%98%B2",
      "layoutProperties": {
        "primaryColor": "#ff7463",
        "secondaryColor": "#5575ff"
      },
      "openLinkUrlPrefixes": [
        "https://mail.google.com/",
        "*"
      ]
    },
    "gmail": {
      "homepageTrigger": {
        "enabled": false
      },
      "composeTrigger": {
        "draftAccess": "METADATA",
        "selectActions": [
          {
            "runFunction": "onGmailCompose",
            "text": "Gmail Draft Checker"
          }
        ]
      }
    }
  }
}
